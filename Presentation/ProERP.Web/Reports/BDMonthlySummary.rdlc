  <Code>Public Function ChangeTimeFormat(ByVal t As UInteger ) As String  

Return (Floor(t/ 3600000) ).ToString()  &amp; ":"&amp; 
	   Format(DateAdd("s", t/1000,"00:00"), "mm")

End Function  

Public Function ChangeSeconds(ByVal t As UInteger ) As String  

